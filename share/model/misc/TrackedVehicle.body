    elements: *WeelShape  
  -
    name: WHEEL_L3
    parent: ROOT
    jointType: revolute
    jointAxis: [ 0, 1, 0 ]
    jointId: 3
    translation: [ -1.2, 1.1, -0.15 ]
    centerOfMass: [ 0, 0, 0 ]
    mass: 100
    inertia: [
      1.3125, 0, 0,
      0, 1.125, 0,
      0, 0, 1.3125 ] 
    import: *wheelHingeParam
    elements: *WeelShape
  -
